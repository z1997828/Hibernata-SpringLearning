<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="counter1" class="ch02._02_constructor._03.Counter">
		<constructor-arg value='10' type='java.lang.Integer' />
	</bean>
	
	<bean id="counter2" class="ch02._02_constructor._03.Counter">
		<constructor-arg value='20' type='java.lang.Long' />
	</bean>
	
</beans>
