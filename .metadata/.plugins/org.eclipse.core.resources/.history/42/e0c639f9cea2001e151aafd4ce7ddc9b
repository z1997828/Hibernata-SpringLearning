package ch01.traditional.model;

public class Master { 					// 飼養Dog寵物的Master類別
	Dog dog;

	public Master() {
		this.dog = new Dog("史奴比"); 	// 緊密耦合
	}

	public void keepPet() {
		dog.smell();
		dog.run();
	}
}

