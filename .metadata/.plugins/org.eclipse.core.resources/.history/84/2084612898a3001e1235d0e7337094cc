@PropertySource("classpath:EEIT73/ch03_EEIT73.properties")

public class JavaConfig {
	
	String inFileName;
	String outFileName;
	
	@Bean
	public File infile() {
		File file = new File(inFileName);
		
		return null;
		
	}
	
}
